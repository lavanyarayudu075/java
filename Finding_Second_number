import java.util.Scanner;
public class SecondNumber
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int x = sc.nextInt();
        int y = sc.nextInt();
        int res;
        res = (2*x-y);
        System.out.println(""+res);
        sc.close();
    }
}
